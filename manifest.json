{
    "manifest_version": 2,
    "name": "Test Fill (V2)",
    "version": "1.0",
    "description": "Adds test fill data",
    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*"],
            "js": ["content-scripts/test-fill.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup/test-fill.html"
    }
}