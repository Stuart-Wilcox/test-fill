{
    "name": "Test Fill",
    "version": "1.0",
    "description": "Auto populate a form with dummy data",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"],
        "persitent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://localhost/*", "http://localhost:3000/*"],
        "js": ["contentScript.js"]
    }],
    "manifest_version": 2
}