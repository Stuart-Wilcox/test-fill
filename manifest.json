{
    "manifest_version": 2,
    "name": "Test Fill (V2)",
    "version": "1.0",
    "description": "Adds test fill data",
    "icons": {
        "48": "icons/TFV2_48.png",
        "96": "icons/TFV2_96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.mozilla.org/*"
            ],
            "js": [
                "content-scripts/test-fill.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_popup": "popup/test-fill.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "stuart_wilcox@outlook.com"
        }
    }
}